{
  "name": "tree-sitter-buildfile",
  "module": "index.js",
  "type": "commonjs",
  "devDependencies": {
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "nan": "^2.18.0",
    "tree-sitter-cli": "^0.20.8"
  },
  "main": "bindings/node",
  "tree-sitter": [
    {
      "scope": "source.build2.buildfile",
      "file-types": [
        "buildfile",
        "root.build",
        "export.build",
        "config.build",
        "bootstrap.build"
      ],
      "injection-regex": "build2.buildfile"
    }
  ]
}
